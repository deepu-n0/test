#include <stdio.h>

int main() {
    int num1, num2;

    // Input two numbers
    printf("Enter the first number: ");
    scanf("%d", &num1);
    printf("Enter the second number: ");
    scanf("%d", &num2);

    // Compare the two numbers and print the largest
    if (num1 > num2) {
        printf("The largest number is: %d\n", num1);
    } else {
        printf("The largest number is: %d\n", num2);
    }

    return 0;
}
